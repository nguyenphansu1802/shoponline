<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Access"
        app:layout_constraintStart_toStartOf="@+id/btn_khac"
        app:layout_constraintTop_toBottomOf="@+id/btn_khac" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="15dp"
        android:text="Equipment"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_equipment"
        app:layout_constraintTop_toBottomOf="@+id/btn_equipment" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="15dp"
        android:text="Fruit"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_fruit"
        app:layout_constraintTop_toBottomOf="@+id/btn_fruit" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="413dp"
        android:layout_height="43dp"
        android:background="#DF0C0C"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageButton
            android:id="@+id/btn_infuser_homepage"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/btn_test_image"
              />

        <TextView
            android:id="@+id/tv_nameuser_homepage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="14dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:textStyle="bold|italic"/>


        <Button
            android:id="@+id/btn_cart_homepage"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:layout_marginLeft="200dp"
            android:background="@drawable/btn_cart"
            android:drawableBottom="@drawable/homepage_cart" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="411dp"
        android:layout_height="40dp"
        android:background="#DF0C0C"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0">

        <EditText
            android:id="@+id/et_findHomepage"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:allowUndo="true"
            android:background="@drawable/button_edit"
            android:hint="Search for product..."
            android:padding="10dp"
            android:textSize="15dp"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/btn_seachshomepage"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:drawableBottom="@drawable/seach_homepage"
            android:background="@drawable/btn_inf_user"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="410dp"
        android:layout_height="163dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/background" />
    </LinearLayout>

    <Button
        android:id="@+id/btn_meat"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="55dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_meat"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_vergetable"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <Button
        android:id="@+id/btn_fish"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="55dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_seafood"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_meat"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <Button
        android:id="@+id/btn_fruit"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_apple"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <Button
        android:id="@+id/btn_vergetable"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="55dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_vegetable"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_fruit"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <Button
        android:id="@+id/btn_fastfood"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/homapage_fastfood"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_fish" />

    <Button
        android:id="@+id/btn_household"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_household"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_fastfood"
        app:layout_constraintTop_toBottomOf="@+id/btn_vergetable" />

    <Button
        android:id="@+id/btn_equipment"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_equipment"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_household"
        app:layout_constraintTop_toBottomOf="@+id/btn_fruit" />

    <Button
        android:id="@+id/btn_other"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/homepage_other"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_equipment"
        app:layout_constraintTop_toBottomOf="@+id/btn_fish" />

    <Button
        android:id="@+id/btn_child"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/ic_baseline_child_care_24"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_fastfood" />

    <Button
        android:id="@+id/btn_ct"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/ic_baseline_insert_comment_24"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_child"
        app:layout_constraintTop_toBottomOf="@+id/btn_household" />

    <Button
        android:id="@+id/btn_infuser_bottom"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:drawableBottom="@drawable/ic_baseline_how_to_reg_24"
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_ct"
        app:layout_constraintTop_toBottomOf="@+id/btn_equipment" />

    <Button
        android:id="@+id/btn_khac"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:text=""
        android:background="@drawable/button_design"
        app:layout_constraintStart_toEndOf="@+id/btn_infuser_bottom"
        app:layout_constraintTop_toBottomOf="@+id/btn_other" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="15dp"
        android:text="Vegetable"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_vergetable"
        app:layout_constraintTop_toBottomOf="@+id/btn_vergetable" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Meat"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_meat"
        app:layout_constraintTop_toBottomOf="@+id/btn_meat" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Sea Food"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_fish"
        app:layout_constraintTop_toBottomOf="@+id/btn_fish" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="15dp"
        android:text="Fast Food"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_fastfood"
        app:layout_constraintTop_toBottomOf="@+id/btn_fastfood" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:text="Household"
        app:layout_constraintStart_toStartOf="@+id/btn_household"
        app:layout_constraintTop_toBottomOf="@+id/btn_household" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:text="Other"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_other"
        app:layout_constraintTop_toBottomOf="@+id/btn_other" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Child"
        app:layout_constraintStart_toStartOf="@+id/btn_child"
        app:layout_constraintTop_toBottomOf="@+id/btn_child" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Report"
        app:layout_constraintStart_toStartOf="@+id/btn_ct"
        app:layout_constraintTop_toBottomOf="@+id/btn_ct" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:text="Information"
        app:layout_constraintStart_toStartOf="@+id/btn_infuser_bottom"
        app:layout_constraintTop_toBottomOf="@+id/btn_infuser_bottom" />
</androidx.constraintlayout.widget.ConstraintLayout>
